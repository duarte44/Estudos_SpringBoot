package com.project.first.project.entities;

import java.io.Serializable;
import java.util.ArrayList;
import java.util.Double;
import java.util.List;
import java.util.Objects;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.ManyToMany;
import javax.persistence.Table;

import com.fasterxml.jackson.annotation.JsonFormat;

@Entity
@Table(name = "tb_notas")
public class Notas implements Serializable {

	private static final long serialVersionUID = 1L;

	@Id // chave primaria
	@GeneratedValue(strategy = GenerationType.IDENTITY) // auto increment no banco
	private Integer id;

	private Double value;

	
	private Double date;

	
	@ManyToMany(mappedBy="notas")
	private List<User> user = new ArrayList<>();

	public Notas() {

	}

	public Notas(Integer id, Double value, Double date) {
		super();
		this.id = id;
		this.value = value;
		this.date = date;		
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Double getValue() {
		return value;
	}

	public void setValue(Double value) {
		this.value = value;
	}

	public Double getDouble() {
		return date;
	}

	public void setDouble(Double date) {
		this.date = date;
	}	

	public List<User> getUser() {
		return user;
	}

	public void setUser(List<User> user) {
		this.user = user;
	}

	@Override
	public int hashCode() {
		return Objects.hash(id);
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Notas other = (Notas) obj;
		return Objects.equals(id, other.id);
	}

}
